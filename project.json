{
  "version": "1.0.0-alpha.0",
  "title": "dotnet-tools-cli",
  "buildOptions": {
    "debugType": "portable",
    "emitEntryPoint": true,
    "compile": {
      "include": [
        "src/**/*.cs"
      ],
      "exclude": [
        "src/**/*.spec.cs"
      ]
    }
  },
  "dependencies": {},
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": "dnxcore50"
    }
  }
  ,
  "configurations": {
    "Test": {
      "buildOptions": {
        "compile": {
          "include": [
            "src/**/*.spec.cs"
          ]
        }
      }
    }
  }
}
